    "@vitest/coverage-v8": "^4.0.6",
    "@vitest/ui": "^4.0.6",
    "jsdom": "^27.1.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.12",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^4.0.6",
