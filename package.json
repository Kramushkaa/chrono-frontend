  "description": "Frontend for Хронониндзя (Chrononinja) historical timeline",
  "private": true,
  "dependencies": {
    "@chrononinja/dto": "file:../chronoline-backend-only/shared-dto",
    "@tanstack/react-query": "^5.51.1",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.9.2",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
